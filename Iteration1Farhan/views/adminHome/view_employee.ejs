<!DOCTYPE html>
<html>
<head>
	<title>Employee List</title>
</head>
<body>
	<div>
			<h2>Employee List</h2> 		
	</div>

	<a href="/adminHome">Back</a>|
	<a href="/logout">Logout</a><br>

	<table border="1">
		<tr>
			<td>Employee Id</td>
			<td>Employee Name</td>
			<td>Employee Email</td>
			<td>Employee Password</td>
			<td>Employee PhoneNo</td>
		</tr>
	<% for(var i=0; i < userlist.length; i++){ %>
		<tr>
			<td><%= userlist[i].user_id %></td>
			<td><%= userlist[i].user_name %></td>
			<td><%= userlist[i].user_email %></td>
			<td><%= userlist[i].user_password %></td>
			<td><%= userlist[i].user_phoneno %></td>
			<td>
				<a href="">Deactive</a> | 
				
			</td>
		</tr>
	<%  } %>
	</table>

</body>
</html>